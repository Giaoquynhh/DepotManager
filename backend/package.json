{
  "name": "container-manager-backend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "dev": "nodemon --watch ./ --ext ts --exec ts-node main.ts",
    "start": "node dist/main.js",
    "build": "tsc -p tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:seed": "ts-node ./prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "job:cleanup-holds": "ts-node ./modules/yard/jobs/cleanupExpiredHolds.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@types/xlsx": "^0.0.35",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "csv-stringify": "^6.5.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "xlsx": "^0.18.5",
    "zod": "^4.1.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.14.11",
    "nodemon": "^3.1.4",
    "prisma": "^5.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
